<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F1F1F1"
    android:foregroundTint="#FFFFFF"
    tools:context=".activity_6">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:srcCompat="@drawable/home_page" />
    <ImageView
        android:id="@+id/asuransi"
        android:layout_width="150dp"
        android:layout_height="120dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="400dp"
        app:srcCompat="@drawable/btn_asuransi" />
    <ImageView
        android:id="@+id/dokter"
        android:layout_width="150dp"
        android:layout_height="120dp"
        android:layout_marginLeft="230dp"
        android:layout_marginTop="405dp"
        app:srcCompat="@drawable/btn_dokter" />

    <ImageView
        android:id="@+id/rs"
        android:layout_width="150dp"
        android:layout_height="120dp"
        android:layout_marginLeft="230dp"
        android:layout_marginTop="530dp"
        app:srcCompat="@drawable/btn_rs" />

    <ImageView
        android:id="@+id/artikel"
        android:layout_width="150dp"
        android:layout_height="120dp"
        android:layout_marginLeft="30dp"
